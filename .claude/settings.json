{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "mcp__memory__*"
    ],
    "defaultMode": "default"
  },
  "model": "sonnet",
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/check-destructive"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks on-start 2>/dev/null",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.file_path // empty' | grep -E '\\.md$' | xargs -I {} prettier --write \"{}\""
          }
        ]
      },
      {
        "matcher": "Write|Edit",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.file_path // empty' | grep -E '\\.go$' | xargs -I {} gofmt -w \"{}\""
          }
        ]
      },
      {
        "matcher": "Write|Edit",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.file_path // empty' | grep -E '\\.rs$' | xargs -I {} rustfmt \"{}\""
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks on-stop 2>/dev/null",
            "timeout": 5
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks advise 2>/dev/null",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUseFailure": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks on-failure 2>/dev/null",
            "timeout": 5
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "auto",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks pre-compact 2>/dev/null",
            "timeout": 3
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/tslater/.claude/bin/memory-hooks on-end 2>/dev/null",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "node ~/.claude/hud/omc-hud.mjs"
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": false,
    "context7@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "elements-of-style@superpowers-marketplace": false,
    "frontend-design@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "claude-md-management@claude-plugins-official": true,
    "claude-code-setup@claude-plugins-official": true,
    "flow@local": true,
    "bach@local": true,
    "episodic-memory@superpowers-marketplace": true,
    "commit-commands@claude-code-plugins": true,
    "frontend-design@claude-code-plugins": true,
    "agent-sdk-dev@claude-code-plugins": true,
    "plugin-dev@claude-code-plugins": true,
    "duet@local": true,
    "documents@wookstar-claude-plugins": true,
    "ffmpeg@wookstar-claude-plugins": true
  },
  "alwaysThinkingEnabled": true,
  "skipDangerousModePermissionPrompt": true,
  "plugins": [
    "/Users/tslater/dev/flow/.claude-plugin"
  ],
  "mcpServers": {
    "memory": {
      "command": "/Users/tslater/.claude/bin/memory"
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    }
  },
  "feedbackSurveyState": {
    "lastShownTime": 1765978818628
  },
  "teammateMode": "tmux"
}
